{
  "extends": [
    "eslint:recommended"
  ],
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "es6": true
  },
  "rules": {
    "no-unused-vars": [2, {"vars": "all", "argsIgnorePattern": "^_"}],
    "no-unexpected-multiline": 2,
    "no-param-reassign": 2,
    "dot-notation": 2,
    "eqeqeq": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 2,
    "no-var": 2,
    "no-use-before-define": 2,
    "no-const-assign": 2,
    "semi": [2, "always"],
    "key-spacing": 2,
    "padded-blocks": [2, "never"],
    "quote-props": [2, "as-needed"],
    "indent": [2, 2]
  }
}
